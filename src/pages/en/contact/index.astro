---
import Layout from "../../../layouts/Layout.astro";
import ContactForm from "../../../components/react/ContactForm";
import Hero from "../../../components/astro/Hero.astro";
const english = await import(`../../../i18n/form/en.json`);
const heroTranslations = await import(`../../../i18n/hero/en.json`);
export const prerender = true;
---

<Layout title="Behm Digital - Contact">
  <Hero heroTranslations={heroTranslations.default.hero} page="contact" />
  <ContactForm client:load translations={english.default} />
</Layout>
